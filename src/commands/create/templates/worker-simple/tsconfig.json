{
	"compilerOptions": {
		"moduleResolution": "Bundler",
		"target": "ESNext",
		"module": "ESNext",
		"strict": true,
		"verbatimModuleSyntax": true,
		"allowJs": true,
		"checkJs": true,
		"resolveJsonModule": true,
		"types": ["@cloudflare/workers-types"]
	},
	"include": ["src/**.ts", "src/**.js"],
	"exclude": ["node_modules/**"]
}
